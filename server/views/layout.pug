doctype html
html
  head
    //- Window/Tab Title
    title= title

    // JQuery CDN
    script(src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js")
    
    // Chart.js CDN
    script(src="https://cdn.jsdelivr.net/npm/chart.js@2.9.3/dist/Chart.min.js")

    // Bootstrap CDN
    link(rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous")
    
    // Google Fonts CDN "Raleway", "Rubik", "Roboto Condensed"
    link(rel="preconnect" href="https://fonts.gstatic.com")
    link(href="https://fonts.googleapis.com/css2?family=Raleway:wght@100&family=Roboto+Condensed:wght@300&family=Rubik:wght@300&display=swap" rel="stylesheet")
    
    // project CSS stylesheet
    link(rel='stylesheet', href='/stylesheets/style.css')
  
  body
    //- Header of page - NASA NTTP Logo
    include components/header
    
    //- Dynamic Page Content
    block content
    
    //- Footer of page - Disclaimer and personal info
    include components/footer
